<?xml version="1.0" encoding="utf-8"?>
<Setting xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <!-- WiFi透传 TCP Server IP地址 -->
  <TCPServerIP>10.10.100.254</TCPServerIP>
  <!-- WiFi透传 TCP Server 端口号 -->
  <TCPServerPort>8899</TCPServerPort>
  <!-- 扫码枪串口号，若为空表示不使用串口扫码枪 -->
  <ScannerPort>COM5</ScannerPort>
  <!-- 扫码枪波特率 -->
  <ScannerBaud>9600</ScannerBaud>
  <!-- 温度采样时间间隔，单位ms -->
  <Interval>500</Interval>
  <!-- 温度采样总时间，单位ms -->
  <TotalTime>10000</TotalTime>
</Setting>