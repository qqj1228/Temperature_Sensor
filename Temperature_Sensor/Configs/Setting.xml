<?xml version="1.0" encoding="utf-8"?>
<Setting xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <!-- WiFi透传 TCP Server IP地址 -->
  <TCPServerIP>10.10.100.254</TCPServerIP>
  <!-- WiFi透传 TCP Server 端口号 -->
  <TCPServerPort>8899</TCPServerPort>
  <!-- 扫码枪串口号，若为空表示不使用串口扫码枪 -->
  <ScannerPort>COM5</ScannerPort>
  <!-- 扫码枪波特率 -->
  <ScannerBaud>9600</ScannerBaud>
  <!-- 温度采样时间间隔，单位ms -->
  <Interval>500</Interval>
  <!-- 温度采样总时间，单位ms -->
  <TotalTime>10000</TotalTime>
  <!-- 数据库IP地址 -->
  <DBIP>127.0.0.1</DBIP>
  <!-- 数据库端口号 -->
  <DBPort>1433</DBPort>
  <!-- 数据库用户名 -->
  <DBPort>sa</DBPort>
  <!-- 数据库密码 -->
  <DBPort>sh49</DBPort>
  <!-- 温度设定值，用于计算设定温度，小于1为比率，大于1为绝对值 -->
  <SetupValue>0.25</SetupValue>
  <!-- 空调工作模式 -->
  <Cooling>true</Cooling>
  <!-- 用哪个温度点作为评判标准，0：全部，1：温度点1，2：温度点2 -->
  <Temper>0</Temper>
  <!-- 是否合格的评判标准，1：平均值，2：连续点 -->
  <Rule>1</Rule>
  <!-- 连续点设定值，小于1为比率，大于1为绝对值 -->
  <SuccessiveValue>0.5</SuccessiveValue>
</Setting>